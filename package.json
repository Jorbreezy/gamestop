{
  "name": "gamestop",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "dotenv node ./bin/www",
    "dev": "dotenv DEBUG=* nodemon ./bin/www",
    "db:migrate": "dotenv knex migrate:latest",
    "db:seeds": "dotenv knex seed:run",
    "db:setup": "dotenv npm run db:migrate && npm run db:seeds",
    "db:rollback": "dotenv knex migrate:rollback"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "knex": "^0.21.12",
    "morgan": "~1.9.1",
    "pg": "^8.5.0",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
